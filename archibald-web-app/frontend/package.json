{
  "name": "archibald-mobile",
  "version": "1.0.0",
  "description": "PWA mobile per inserimento ordini Archibald",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:typecheck": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
